<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.wso2.carbon.identity.workflow.mgt.api.v1.impl.WorkflowApiServiceImpl"/>
    <bean class="org.wso2.carbon.identity.workflow.mgt.api.v1.core.WorkflowManagementService"/>

    <bean id="WorkflowMgtServiceFactoryBean"
          class="org.wso2.carbon.identity.workflow.mgt.api.common.factory.WorkflowManagementOSGIServiceFactory"/>
    <bean id="WorkflowManagementServiceHolderBean"
          class="org.wso2.carbon.identity.workflow.mgt.api.common.WorkflowManagementServiceHolder">
        <property name="workFlowExecutorManager" ref="WorkflowMgtServiceFactoryBean"/>
    </bean>
</beans>
